    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-typescript": "^0.14.0",
    "faker": "^4.1.0",
    "jest": "^24.0.0",
    "jest-mock": "^24.8.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
